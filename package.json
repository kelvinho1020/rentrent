{
  "name": "rentrent",
  "version": "1.0.0",
  "private": true,
  "description": "以通勤時間為核心的租屋搜尋平台",
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "frontend": "pnpm --filter frontend run dev",
    "backend": "pnpm --filter rentrent-backend run dev",
    "backend:build": "pnpm --filter rentrent-backend run build",
    "backend:start": "pnpm --filter rentrent-backend run start",
    "backend:migrate": "pnpm --filter rentrent-backend run migrate",
    "backend:seed": "pnpm --filter rentrent-backend run seed",
    "crawler": "cd apps/crawler && python crawler.py",
    "crawler:update": "cd apps/crawler && python daily_update.py",
    "crawler:test": "cd apps/crawler && python daily_update.py --test",
    "docker:infra": "docker-compose up -d db redis",
    "docker:all": "docker-compose --profile crawler up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "setup": "./scripts/setup.sh"
  },
  "engines": {
    "node": "20.10.0",
    "pnpm": "8.6.12"
  },
  "packageManager": "pnpm",
  "author": "",
  "license": "MIT"
} 
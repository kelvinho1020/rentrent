{
  "name": "rentrent-backend",
  "version": "1.0.0",
  "description": "RentRent 後端服務 - Express/TypeScript/PostgreSQL/Redis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.scripts.json",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "migrate:diff": "prisma migrate diff",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:seed": "prisma db seed",
    "seed": "ts-node src/utils/seed.ts",
    "import-listings": "ts-node scripts/import-listings.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@rentrent/shared": "workspace:*",
    "@types/multer": "^1.4.13",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.7",
    "@types/supertest": "^2.0.15",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "prisma": "^5.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "20.10.0"
  }
}
